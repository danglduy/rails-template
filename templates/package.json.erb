{
  "name": "<%= options[:app_name] %>",
  "private": true,
  "scripts": {
    "js:watch": "webpack --watch --config ./config/webpack/development.js",
    "js:dist": "webpack --config ./config/webpack/production.js",
    "js:test": "karma start --single-run",
    "js:test:watch": "karma start"
  },
  "devDependencies": {
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-qunit": "1.2.1",
    "karma-safari-launcher": "1.0.0",
    "karma-webpack": "2.0.4",
    "qunitjs": "2.4.0",
    "webpack-rails": "https://github.com/fnando/webpack-rails.git"
  },
  "dependencies": {
    <%- unless options[:skip_turbolinks] -%>
    "turbolinks": "^5.0.3"
    <%- end -%>
  }
}
